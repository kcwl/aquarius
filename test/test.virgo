struct person
{
	bool sex;
	uint32 addr;
	int32 age;
	uint64 telephone;
	int64 score;
	float hp;
	double mana;
	string name;
	bytes orders;
}

message login:tcp -> 10001
{
	request
	{
		person per_req;
	}
	response
	{
		person per_resp;
	}
}

message new_http_login:http[post] -> /login/new
{
	request
	{
		int32 uuid;
		person per_req;
	}
	response
	{
		int32 uuid;
		person per_resp;
	}
}

message http_test_get:http[get] -> /login
{
	request
	{
		int32 user;
		string passwd;
	}
	response
	{
		int32 user;
		string passwd;
	}
}