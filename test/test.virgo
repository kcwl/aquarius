struct person
{
	bool sex;
	uint32 addr;
	int32 age;
	uint64 telephone;
	int64 score;
	float hp;
	double mana;
	string name;
	bytes orders;
}

message new_login -> tcp-key:10001
{
	request{
		int32 uuid;
	}
	response{
		person per_resp;
	}
}

message new_http_login -> http-post:/login
{
	request{
		int32 uuid;
	}
	response{
		person per_resp;
	}
}

protocol login -> tcp-key:10001
{
	request {
	header:
		int32 uuid;
	body:
		person per_req;
	}
	response {
	header:
		int32 uuid;
	body:
		person per_resp;
	}
}

protocol http_login -> http-post:/login
{
	request {
	header:
		int32 uuid;
	body:
		person per_req;
	}
	response {
	header:
		int32 uuid;
	body:
		person per_resp;
	}
}