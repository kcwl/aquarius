struct person
{
	bool sex;
	uint32 addr;
	int32 age;
	uint64 telephone;
	int64 score;
	float hp;
	double mana;
	string name;
	bytes orders;
}

message login_request -> tcp-binary:10001
{
	person per_req;
}

message login_response -> tcp-binary:10001
{
	person per_resp;
}

message new_http_login_request -> http-post:login/new
{
	int32 uuid;
	person per_req;
}
message new_http_login_response -> http-post:login/new
{
	int32 uuid;
	person per_resp;
}

message http_test_get_request -> http-get:login
{
	int32 user;
	string passwd;
}

message http_test_get_response -> http-get:login
{
	int32 user;
	string passwd;
}

protocol tcp_login -> tcp-binary:10002
{
	request {
	header:
		int32 uuid;
	body:
		person per_req;
	}
	response {
	header:
		int32 uuid;
	body:
		person per_resp;
	}
}

protocol http_login -> http-post:login
{
	request {
	header:
		int32 uuid;
	body:
		person per_req;
	}
	response {
	header:
		int32 uuid;
	body:
		person per_resp;
	}
}